package day10;

public class Product_of_digit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int number = 55; 
        int product = 1;

        while (number != 0) 
        	
        {
            int digit = number % 10; 
            product *= digit;          
             number /=10;          
        }

        System.out.println("Sum of digits: " + product);
    }
		
		
	}


