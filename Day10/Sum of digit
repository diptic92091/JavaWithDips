package day10;

public class Sum_of_digit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		        int number = 55; 
		        int sum = 0;

		        while (number != 0) 
		        {
		            int digit = number % 10;
		            sum += digit;           
		            number /= 10;           
		        }

		        System.out.println("Sum of digits: " + sum);
		    }
		


	}


