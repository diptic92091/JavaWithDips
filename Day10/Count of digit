package day10;

public class Count_of_digit {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		        int number = 12345; 
		        int count = 0;

		        while (number != 0) 
		        {
		            count++;
		            number /= 10; 
		        }

		        System.out.println("Number of digits: " + count);
		    }
				
		
	}

