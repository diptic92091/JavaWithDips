package day2;

public class Skipping_number {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	
		        int i = 1;

		        while (i <= 10) {
		                                         
		            if (i >= 6 && i <= 8)             // Skip numbers from 6 to 8
		            
		            {
		                i++;
		                continue;
		            }
		            System.out.println(i);
		            i++;
		        }
		    }
	

		
	}
