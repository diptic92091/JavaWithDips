package day5;

class A
{
	int a=20;
	void display()
	{
		System.out.println(a);
		
		
	}
}
class B extends A
{
	int b=30;
	void sum()
	{
		
		System.out.println(b);
	}
}

public class Single_inheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		B extd = new B();
		extd.display();
		
		extd.sum();
		
	}

}
