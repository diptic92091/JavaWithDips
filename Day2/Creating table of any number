package day2;

public class Multiplication_Table {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 9; 
        System.out.println("Multiplication Table for " + a);

        
        for (int i = 1; i <= 10; i++) {
            System.out.println(a + " x " + i + " = " + (a* i));
        }

        
	}

}
