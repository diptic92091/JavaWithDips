package day4;

public class This_method {

    class Person {
        String name;

                                     
        Person(String name)
        {
            this.name = name; 
        }

        
        void display() {
            System.out.println("Name: " + this.name);  
        }
    }

    public static void main(String[] args) {
     
        This_method outerClass = new This_method();

        This_method.Person person1 = outerClass.new Person("Dipti");

        person1.display();
    }
}
